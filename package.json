{
    "name": "metal-syntax",
    "displayName": "MSL Syntax Highlighting",
    "description": "Basic language support for Metal Shader Language",
    "version": "1.27.1",
    "icon": "icon.png",
    "scripts": {},
    "keywords": [
        "C++",
        "cpp",
        "syntax",
        "textmate",
        "highlighting",
        "coloring",
        "color"
    ],
    "engines": {
        "vscode": "^1.0.0"
    },
    "author": {
        "name": "dontwanttothink"
    },
    "publisher": "dontwantttothink",
    "categories": [
        "Programming Languages"
    ],
    "repository": {
        "url": "https://github.com/jeff-hykin/better-cpp-syntax",
        "type": "git"
    },
    "contributes": {
        "languages": [
            {
                "id": "metal",
                "aliases": [
                    "Metal Shader Language",
                    "Metal",
                    "MSL"
                ],
                "extensions": [
                    "metal"
                ],
                "configuration": "./language-configuration.json"
            }
        ],
        "grammars": [
            {
                "language": "metal",
                "scopeName": "source.cpp.embedded.macro",
                "path": "./autogenerated/cpp.embedded.macro.tmLanguage.json",
                "embeddedLanguages": {
                    "meta.embedded.assembly.cpp": "asm"
                }
            },
            {
                "language": "metal",
                "scopeName": "source.cpp",
                "path": "./autogenerated/cpp.tmLanguage.json",
                "embeddedLanguages": {
                    "meta.embedded.assembly.cpp": "asm",
                    "meta.preprocessor.macro.contents": "cpp"
                }
            }
        ]
    },
    "devDependencies": {
        "chalk": "^2.4.2",
        "dictionary-en-us": "^2.1.1",
        "lodash": "^4.17.19",
        "nspell": "^2.1.2",
        "oniguruma": "^7.2.0",
        "ovsx": "^0.5.1",
        "textmate-bailout": "^1.1.0",
        "textmate-tester": "^1.1.9",
        "vsce": "^1.103.1"
    }
}
